{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Deno",
            "type": "pwa-node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect=127.0.0.1:9229",
                "--allow-all",
                "src/main.ts"
            ],
            "attachSimplePort": 9229,
            "port": 9229,
            "outputCapture": "std",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
